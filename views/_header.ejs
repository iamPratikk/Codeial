<header>
  <div id="header-parent">
    <h1>
      <a href="/">Codeial</a>
    </h1>
    <div id="sign_up_links">
      <ul>
        <%if(locals.user){ %>
        <li><a href="/users/profile/<%=user._id%>"><%= user.name%></a></li>
        <li>
          <a href="/users/sign-out">Sign-Out</a>
        </li>
        <%}else{ %>
        <li><a href="/users/sign-in">Sign-In</a></li>
        <li><a href="/users/sign-up">Sign-Up</a></li>
        <% } %>
      </ul>
    </div>
  </div>
</header>
